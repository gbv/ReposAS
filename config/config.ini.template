[General Config]
; Enable parsing old MIR logs before css load in detailed view
oldMirLogs = false
; get-method for metadata (file - access datadir, http - uses recive and Restapi)
getmethod = file
; Datadir - location of XML metadata
datadir = ../data
; mycoreIdPattern - regular Expression to much the mycoreId (default: ([^\/]+_[^\/]+_[0-9]{8})) 
mycoreIdPattern = ([^\/]+_[^\/]+_[0-9]{8})
; derivateIdPattern - regular Expression to much the derivateId (default: ([^\/]+_derivate_[0-9]{8})) 
derivateIdPattern = ([^\/]+_derivate_[0-9]{8})
